<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="calendar_active_month_bg">#ffffffff</color>
    <color name="calendar_bg">#ffffffff</color>
    <color name="calendar_divider">@android:color/transparent</color>
    <color name="calendar_highlighted_day_bg">@android:color/transparent</color>
    <color name="calendar_inactive_month_bg">@android:color/transparent</color>
    <color name="calendar_selected_day_bg">#263238</color>
    <color name="calendar_selected_range_bg">#263238</color>
    <color name="calendar_text_active">#263238</color>
    <color name="calendar_text_highlighted">#263238</color>
    <color name="calendar_text_inactive">#40778088</color>
    <color name="calendar_text_selected">#9f9f9f</color>
    <color name="calendar_text_today">#ff3333</color>
    <color name="calendar_text_unselectable">#263238</color>
    <declare-styleable name="CalendarPickerView"><attr name="android:background"/><attr format="color" name="tsquare_dividerColor"/><attr format="reference" name="tsquare_dayBackground"/><attr format="color" name="tsquare_dayTextColor"/><attr format="color" name="tsquare_titleTextColor"/><attr format="boolean" name="tsquare_displayHeader"/><attr format="color" name="tsquare_headerTextColor"/></declare-styleable>
    <declare-styleable name="calendar_cell"><attr format="boolean" name="tsquare_state_selectable"/><attr format="boolean" name="tsquare_state_current_month"/><attr format="boolean" name="tsquare_state_today"/><attr format="boolean" name="tsquare_state_range_first"/><attr format="boolean" name="tsquare_state_range_middle"/><attr format="boolean" name="tsquare_state_range_last"/><attr format="boolean" name="tsquare_state_highlighted"/></declare-styleable>
    <dimen name="calendar_day_headers_paddingbottom">4dp</dimen>
    <dimen name="calendar_month_title_bottommargin">4dp</dimen>
    <dimen name="calendar_month_topmargin">16dp</dimen>
    <dimen name="calendar_text_medium">18sp</dimen>
    <dimen name="calendar_text_small">14sp</dimen>
    <string name="day_name_format">EEE</string>
    <string name="invalid_date">Date must be between %1$s and %2$s.</string>
    <string name="month_name_format">yyyy-MM</string>
    <style name="CalendarCell">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_weight">1</item>
    <item name="android:gravity">center</item>
    <item name="android:textAlignment" ns1:targetApi="jelly_bean_mr1">center</item>
  </style>
    <style name="CalendarCell.CalendarDate">
    <item name="android:textSize">@dimen/calendar_text_medium</item>
    <item name="android:clickable">true</item>
  </style>
    <style name="CalendarCell.DayHeader">
    <item name="android:textSize">@dimen/calendar_text_small</item>
  </style>
    <style name="CalendarTitle">
    <item name="android:textSize">@dimen/calendar_text_medium</item>
  </style>
</resources>